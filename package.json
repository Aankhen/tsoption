{
  "name": "tsoption",
  "version": "0.4.1",
  "description": "Correct, easy to use Option type for TypeScript",
  "keywords": [
    "adt",
    "algebra",
    "algebraic data type",
    "haskell",
    "option",
    "optional",
    "maybe",
    "monad",
    "fantasy",
    "fantasyland",
    "functor",
    "sanctuary",
    "scala",
    "typescript",
    "type",
    "typesafe",
    "null",
    "safe"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcherny/tsoption.git"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && tsc -d",
    "clean": "rm -f ./index.d.ts ./test.d.ts ./*.js.map ./*.js",
    "lint": "tslint *.ts",
    "prepublish": "npm test",
    "pretest": "npm run build",
    "tdd": "npm run clean && concurrently -k 'npm run test:watch' 'npm run watch'",
    "test": "ava",
    "test:watch": "ava -w",
    "watch": "tsc -w"
  },
  "author": "Boris Cherny <boris@performancejs.com> (https://performancejs.com)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "concurrently": "^3.5.1",
    "fantasy-land": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {}
}
